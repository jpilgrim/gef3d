<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.eclipse.gef3d.examples.ecore.diagram.part.EcoreDiagramEditor3D"
            default="true"
            extensions="ecorediag"
            id="org.eclipse.gef3d.examples.editor.EcoreDiagramEditor3D"
            name="Ecore Tools Editor 3D">
      </editor>
   </extension>
   
   
   <extension point="org.eclipse.ui.editors">
      <editor
            class="org.eclipse.gef3d.examples.ecore.diagram.part.MultiEcoreGraphicalEditor3D"
            default="false"
            extensions="ecorediag"
            id="org.eclipse.gef3d.examples.ecore.diagram.part.MultiEcoreGraphicalEditor3D"
            name="Multi Ecore Tools Editor 3D">
      </editor>
   </extension>
   
   <extension
         point="org.eclipse.gmf.runtime.diagram.ui.editpartProviders">
      <editpartProvider
            class="org.eclipse.gef3d.examples.ecore.diagram.providers.EcoreEditPartProvider3D">
         <Priority
               name="Highest">
         </Priority>
      </editpartProvider>
   </extension>
</plugin>
